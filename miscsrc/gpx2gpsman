use strict;
use FindBin;
use lib ("$FindBin::RealBin/..",
	 "$FindBin::RealBin/../lib",
	);
use GPS::GpsmanData::Any;

my $file = shift || die "Please specify gpx file";
my $gpsman = GPS::GpsmanData::Any->load($file);
print $gpsman->as_string;

__END__
