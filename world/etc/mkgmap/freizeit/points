# 0x2D08          Ice/Sporting                          0x6504          Bend (Biegung)
# 0x2D09          Swimming                              0x6505          Canal
# 0x2D0A          Sports (point)                        0x6506          Channel
# 0x2D0B          Sailing Airport                       0x6507          Cove (Bucht)
# 0x2E01          Department Store                      0x6508          Falls (großer Wasserfall)
# 0x2E02          Grocery                               0x6509          Geyser
# 0x2E03          General Merchandiser                  0x650A          Glacier
# 0x2E04          Shopping Center                       0x650B          Harbour
# 0x2E05          Pharmacy                              0x650C          Island
# 0x2E06          Convenience                           0x6500          Lake
# 0x2E07          Apparel                               0x650E          Rapids (Stromschnellen)
# 0x2E08          House and Garden                      0x650F          Reservoir
# 0x2E09          Home Furnishing                       0x6510          Sea
# 0x2E0a          Special Retail                        0x6511          Spring
# 0x2E0b          Computer/Software                     0x6512          Stream
# 0x2F00          generic service                       0x6513          Swamp
# 0x2F01          Fuel/Gas                              0x6600          Land Feature
# 0x2F02          Car Rental                            0x6601          Arch (Bogen)
# 0x2F03          Car Repair                            0x6602          Area
# 0x2F04          Airport                               0x6603          Basin
# 0x2F05          Post Office                           0x6600          Beach
# 0x2F06          Bank                                  0x6605          Bench
# 0x2F07          Car Dealer (point)                    0x6606          Cape (Kap)
# 0x2F08          Bus Station                           0x6607          Cliff
# 0x2F09          Marina                                0x6608          Crater
# 0x2F0A          Wrecker Service                       0x6609          Flat (Ebene)
# 0x2F0B          Parking                               0x660A          Forest
# 0x2F0C          Restroom                              0x660B          Gap
# 0x2F0D          Automobile Club                       0x660C          Gut (Meerenge)
# 0x2F0E          Car Wash                              0x660D          Isthmus (Isthmus, Landenge)
# 0x2F0F          Garmin Dealer                         0x660E          Lava
# 0x2F10          Personal Service                      0x660F          Pillar (Säule?)
# 0x2F11          Business Service                      0x6610          Plain (Ebene)
# 0x2F12          Communication                         0x6611          Range (Gebiergskette)
# 0x2F13          Repair Service                        0x6612          Reserve (Schutzgebiet)
# 0x2F14          Social Service                        0x6613          Ridge (Bergzug, Kamm, Grat)
# 0x2F15          Utility                               0x6614          Rock
# 0x2F16          Truck Stop                            0x6615          Slope (Abhang)
# 0x3000          generic emergency/government          0x6616          Summit
# 0x3001          Police Station                        0x6617          Valley
# 0x3002          Hospital                              0x6618          Woods
# 0x3003          Public Office
# 0x3004          Justice
# 0x3005          Concert hall (point)
# 0x3006          Border Station (point)
#
# Standard types - points (laut Thorsten Kukuk, Stand: 01/2012):
# --------------------------------------------------------------
# Attraktionen:
# - Arena/Rennbahn:                      0x2c08
# - Museum/Historisches:                 0x2c02, 0x2c11
# - Park/Garten:                         0x2c00, 0x2c06, 0x491a-0x491f, 0x4a16-0x4a1f, 0x640e
# - Saal/Auditorium:                     0x2c09
# - Sehenswürdigkeit:                    0x2c04, 0x2c07 (Zoo), 0x5200-0x521f
# - Weingut:                             0x2c0a
# 
# Autoservices
# - Automobilclub:                       0x2f0d
# - Autoreparatur:                       0x2f03
# - Autovermietung:                      0x2f02
# - Autowaschanlage:                     0x2f0e
# - Abschleppdienst:                     0x2f0a
# - Händler/Autoteile:                   0x2f07, 0x3100-0x311f
# - Parkplatz:                           0x2f0b, 0x4d00-0x4d1f
# - Rastplatz (Toiletten):               0x2f0c, 0x4e00-0x4e1f
# - Besucherinformation (Rastpl.):       0x4c00-0x4c1f
# - Truckstop:                           0x2f16
# 
# Bank/Geldautomat:                      0x2f06, 0x2f1d, 0x2f1e
# 
# Beförderungsmittel/Verkehrsmittel:
# - Autovermietung:                      0x2f02
# - Bahnhof (Landverkehr):               0x2f08, 0x2f1f
# - Flughafen (Luftverkehr):             0x2f04, 0x5900-0x591f (0x5904 Helipad)
# - Transitservice:                      0x2f17
# 
# Einkaufsmöglichkeiten:
# - Apotheke/Drogerie:                   0x2e05
# - Bedarfsartikel:                      0x2e06
# - Bekleidung:                          0x2e07
# - Computer/Software:                   0x2e0b
# - Einkaufszentrum:                     0x2e04, 0x2e0d (Garmin)
# - Einrichtungsgegenstände:             0x2e09
# - Gemischtwaren:                       0x2e00, 0x2e03
# - Haus und Garten:                     0x2e08
# - Kaufhaus:                            0x2e01
# - Lebensmittel:                        0x2e02
# - Spezialitäteneinzelhandel:           0x2e0a
# 
# Essen & Trinken:
# - Amerikanisch:                        0x2a01, 0x2a1b
# - Cafe:                                0x2a0e
# - Asiatisch:                           0x2a02
# - Chinesisch:                          0x2a04
# - Deutsch:                             0x2a10
# - Englisch:                            0x2a11
# - Feinkostladen:                       0x2a0d
# - Deli/Bäckerei:                       0x2a05
# - Französisch:                         0x2a0f
# - Grill/Barbecue-Restaurant:           0x2a03
# - International:                       0x2a06, 0x2a13-0x2a1f
# - Italienisch:                         0x2a08
# - Pizza:                               0x2a0a
# - Meeresfrüchte:                       0x2a0b
# - Mexikanisch:                         0x2a09
# - Schnellimbiss:                       0x2a07
# - Sonstige:                            0x2a00, 0x4500-0x451f
# - Spezialitäten:                       0x2a12
# - Steak/Grill:                         0x2a0c
# 
# Freizeit:
# - Arena/Rennbahn:                      0x2c08
# - Golfplatz:                           0x2d05, 0x4000-0x401f, 0x2d12 (Garmin), 0x4010-0x401f (Mapsource)
# - Sportflugplatz:                      0x2d0b
# - Park/Garten:                         0x2c06, 0x491a-0x491f, 0x4a16-0x4a1f, 0x640e
# - Skizentrum/Skiort:                   0x2d06, 0x5300-0x531f
# - Bowling-Center:                      0x2d07
# - Eisbahn:                             0x2d08
# - Schwimmbad:                          0x2d09, 0x5400-0x541f
# - Sport/Fitnesscenter:                 0x2d0a, 0x5c00-0x5c1f, 0x2d13 (Garmin)
# - Vergnügungspark/Themenpark:          0x2c01
# 
# Gemeinde:
# - Andachtsort:                         0x2c0b, 0x6404
# - Bibliothek:                          0x2c03
# - Feuerwehr:                           0x3008
# - Gemeindezentrum:                     0x3005, 0x300a (Garmin)
# - Gericht:                             0x3004
# - Grenzübergang:                       0x3006
# - Polizeistation:                      0x3001
# - Postamt:                             0x2f05, 0x640f
# - Rathaus:                             0x3003
# - Regierungsstelle:                    0x3000, 0x3007
# - Schule:                              0x2c05, 0x6410
# - Versorgungsbetrieb:                  0x2f15
# 
# Kraftstoff:
# - Autokraftstoff:                      0x2f01, 0x4400-0x441f
# - Bedarfsartikel:                      0x2e06
# - Truckstop:                           0x2f16
# 
# Krankenhaus:                           0x3002, 0x4b00-0x4b1f, 0x6408
# 
# Sonstige:
# - Dienstleistungen für Einzelpersonen: 0x2f10
# - Dienstleistungen für Unternehmen:    0x2f11
# - GARMIN-Händler:                      0x2f0f
# - Kommunikation:                       0x2f12
# - Reparaturservice:                    0x2f13
# - Sonstige:                            0x2f00
# - Sozialeinrichtungen:                 0x2f14
# - Yachthafen/Reparatur & Lager:        0x2f09, 0x4300-0x431f
# 
# Unterhaltung:
# - Bar/Nachtclub:                       0x2d02, 0x2d0d, 0x2d0e, 0x4600-0x461f
# - Casino:                              0x2d04
# - Kino:                                0x2d03, 0x2d10
# - Saal/Auditorium:                     0x2c09
# - Theater:                             0x2d00, 0x2d01, 0x2d11
# 
# Unterkunft:
# - Campingplatz/Wohnmobil-Park:         0x2b03, 0x2b05, 0x2b06, 0x2b07, 0x4800-0x481f
# - Hotel/Motel:                         0x2b00, 0x2b01, 0x2b02, 0x2b04
#
# Conflict:                              0x2d12, 0x2d13
#
# Garmin BaseCamp (OS X) Suchmöglichkeiten (Filtermöglichkeiten unter Windows):
# -----------------------------------------------------------------------------
# - Städte                                              - Geografische Punkte
# - Alle POIs                                             - Alle Unterkategorien
# - Attraktionen                                          - Künstlich
#   - Alle Unterkategorien                                - Land
#   - Arena / Rennbahn                                    - Wasser
#   - Museum / Historische Sehenswürdigkeiten           - Kraftstoff
#   - Park / Garten                                       - Alle Unterkategorien
#   - Saal / Auditorium                                   - Autokraftstoff
#   - Sehenswürdigkeiten                                  - Bedarfsartikel
#   - Vergnügungspark / Themenpark                        - Truckstop
#   - Weingut                                           - Krankenhaus
# - Autoservices                                        - Marine
#   - Alle Unterkategorien                                - Alle Unterkategorien
#   - Automobilclub                                       - Fischen
#   - Autoreparatur                                       - Fotopunkt
#   - Autovermietung                                      - Gezeitenstation
#   - Autowaschanlage                                     - Hindernis / Gefahr
#   - Bergungsdienst                                      - Kartenpunkt
#   - Händler / Autoteile                                 - Marineservices
#   - Parkmöglichkeiten                                   - Seezeichen
#   - Rastplatz / Besucherinformation                     - Sonstige
#   - Truckstop                                           - Strömungsvorhersagestation
# - Bank / Geldautomat                                    - Tiefe
# - Beförderungsmittel                                  - Restaurants
#   - Alle Unterkategorien                                - Alle Unterkategorien
#   - Autovermietung                                      - Amerikanisch
#   - Landverkehr                                         - Asiatisch
#   - Luftverkehr                                         - Bäckerei
#   - Transitservice                                      - Cafe / Bar
# - Einkaufsmöglichkeiten                                 - Chinesisch
#   - Alle Unterkategorien                                - Deutsch
#   - Apotheke / Drogerie                                 - Englisch
#   - Bedarfsartikel                                      - Feinkostladen / Bäckerei
#   - Bekleidung                                          - Französisch
#   - Computer / Software                                 - Grillen
#   - Einkaufszentrum                                     - International
#   - Einrichtungsgegenstände                             - Italienisch
#   - Gemischtwaren                                       - Meeresfrüchte
#   - Haus und Garten                                     - Mexikanisch
#   - Kaufhaus                                            - Pizzeria
#   - Lebensmittel                                        - Schnellimbiss
#   - Spezialitäteneinzelhandel                           - Sonstige
# - Freizeit                                              - Spezialitäten
#   - Arena / Rennbahn                                    - Steak / Grill
#   - Bowling-Center                                    - Sonstige
#   - Golfplatz                                           - Alle Unterkategorien
#   - Öffentlicher Sportflugplatz                         - Dienstleistungen für Einzelpersonen
#   - Park / Garten                                       - Dienstleistungen für Unternehmen
#   - Skizentrum / Skiort                                 - Garmin-Händler
#   - Sportcenter / Fitness-Center                        - Jachthafen / Reparatur & Lager
#   - Vergnügungspark / Themenpark                        - Kommunikation
# - Gemeinde                                              - Reparaturservice
#   - Alle Unterkategorien                                - Sonstige
#   - Andachtsort                                         - Sozialeinrichtungen
#   - Bibliothek                                        - Unterhaltung
#   - Feuerwehr                                           - Alle Unterkategorien
#   - Gemeindezentrum                                     - Bar / Nachtclub
#   - Gericht                                             - Casino
#   - Grenzübergang                                       - Kino
#   - Polizeistation                                      - Saal / Auditorium
#   - Postamt                                             - Theater
#   - Rathaus                                           - Unterkunft
#   - Regierungsstelle                                    - Alle Unterkategorien
#   - Schule                                              - Bed & Breakfast
#   - Versorgungsbetrieb                                  - Campingplatz / Wohnmobil-Park
#                                                         - Hotel / Motel
#                                                         - Park
#                                                         - Trainingscamp
# Anmerkungen:
# - Die Suchfunktion basiert auf den (durch Garmin) definierten POI-Nummern.
# - D.h. eigene POI-Feature sind "passenden" Garmin-POI-Nummern zuzuordnen.
# - Unterschiedliche POI (für das gleiche Objekt) in verschiedenen Leveln sind nicht möglich.
# - Link auf Mapnik-Symbole: http://svn.openstreetmap.org/applications/rendering/mapnik/symbols/
# - Es sollten nur POIs mit den Subtypen 01-1F verwendet werden (z.B. 0x0001-0x001F oder 0x2400-241F).
# - POIs mit den Subtypen 20-FF (z.B. 0x0020-0x00FF) werden auf vielen Garmin-Geräten nicht angezeigt.
# ------------------------------------------------------------


# -------------------------------------------------
# Adress- / Indexsuche:
# -------------------------------------------------
# first set the country code
mkgmap:country!=* & mkgmap:admin_level2=* { set mkgmap:country='${mkgmap:admin_level2}' }
mkgmap:country!=* & addr:country=* { set mkgmap:country='${addr:country}' }
mkgmap:country!=* & is_in:country=* { set mkgmap:country='${is_in:country}' }

# country specific rules first

# Netherlands = NLD
mkgmap:country=NLD & mkgmap:region!=* & mkgmap:admin_level4=* { set mkgmap:region='${mkgmap:admin_level4}' }
mkgmap:country=NLD & mkgmap:city!=* & mkgmap:admin_level10=* { set mkgmap:city='${mkgmap:admin_level10}' }
mkgmap:country=NLD & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }

# Germany = DEU cities (Wichtig: Die Abarbeitungsreihenfolge (von oben nach unten) ist relevant.)

# Hamburg (Sonderfall):
# Die Bezeichnung "Hamburg" gibt es nur im "admin_level=4".
# Im "admin_level=9" gibt es "Hamburg-Mitte" und "Hamburg-Nord".
# Für die Adresssuche wird deshalb das "admin_level=4" verwendet.
mkgmap:country=DEU & mkgmap:admin_level4=Hamburg {set mkgmap:city='${mkgmap:admin_level4}' }

# Berlin (kein Sonnderfall):
# Die Bezeichnung "Berlin" gibt es nur im "admin_level=4".
# Im "admin_level=6" gibt es die Bezeichnung "Berlin, Stadt".
# Nachfolgend greift die Regel für "admin_level=6".

# Bremen (kein Sonderfall):
# Im "admin_level=4" gibt es die Bezeichnung "Freie Hansestadt Bremen".
# Im "admin_level=6" gibt es die Bezeichnungen "Bremen, Stadt" und "Bremerhafen".
# Nachfolgend greift die Regel für "admin_level=6".

# Diverse Verwaltungseinheiten im "admin_level 8" tragen ein Bezeichnungspräfix.
# Am häufigsten in Deutschland kommen folgende Präfixe vor (Stand 12/2011):
# - "Gemeinde "        (658x, Beispiele: "Gemeinde Spalt", "Gemeinde Maxdorf")
# - "Bad "             (135x, Beispiele: "Bad Brambach", "Bad Ems")
# - "Stadt "            (67x, Beispiele: "Stadt Linz am Rhein", "Stadt Rodgau")
# - "Groß "             (46x, Beispiele: "Groß Dratow", "Groß Wittensee")
# - "Gemeindefreies "   (29x, Beispiele: "Gemeindefreies Gebiet Harz", "Gemeindefreies Gebiet Soos")
# - "Klein "            (25x, Beispiele: "Klein Luckow", "Klein Vielen")
# - "Sankt "            (25x, Beispiele: "Sankt Goarshausen", "Sankt Peter-Ording")
# Die Präfixe "Gemeinde " und "Stadt " stellen keinen Namensbestandteil dar und werden entfernt.

mkgmap:country=DEU & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8|subst:Gemeinde |subst:Stadt }' }
mkgmap:country=DEU & mkgmap:city!=* & mkgmap:admin_level7=* { set mkgmap:city='${mkgmap:admin_level7}' }
mkgmap:country=DEU & mkgmap:city!=* & mkgmap:admin_level6=* { set mkgmap:city='${mkgmap:admin_level6}' }
mkgmap:country=DEU & mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:country=DEU & mkgmap:city!=* & mkgmap:admin_level10=* { set mkgmap:city='${mkgmap:admin_level10}' }

# Österreich = AUT
# Das Präfix "Gemeinde " stellt keinen Namensbestandteil dar und wird entfernt.
mkgmap:country=AUT & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8|subst:Gemeinde }' }

mkgmap:country=BEL & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=CZE & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=CZE & mkgmap:city!=* & mkgmap:admin_level7=* { set mkgmap:city='${mkgmap:admin_level7}' }
mkgmap:country=DNK & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=DNK & mkgmap:city!=* & mkgmap:admin_level7=* { set mkgmap:city='${mkgmap:admin_level7}' }
mkgmap:country=FIN & mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:country=FIN & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=FRA & mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:country=FRA & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=ISL & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=ITA & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=LUX & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=NOR & mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:country=POL & mkgmap:city!=* & mkgmap:admin_level10=* { set mkgmap:city='${mkgmap:admin_level10}' }
mkgmap:country=POL & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=PRT & mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:country=PRT & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=SVN & mkgmap:city!=* & mkgmap:admin_level10=* { set mkgmap:city='${mkgmap:admin_level10}' }
mkgmap:country=ESP & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:country=SWE & mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:country=SWE & mkgmap:city!=* & mkgmap:admin_level7=* { set mkgmap:city='${mkgmap:admin_level7}' }
mkgmap:country=CHE & mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }

# common rules for all the rest of countries
mkgmap:region!=* & mkgmap:admin_level6=* { set mkgmap:region='${mkgmap:admin_level6}' }
mkgmap:region!=* & mkgmap:admin_level5=* { set mkgmap:region='${mkgmap:admin_level5}' }
mkgmap:region!=* & mkgmap:admin_level4=* { set mkgmap:region='${mkgmap:admin_level4}' }
mkgmap:region!=* & mkgmap:admin_level3=* { set mkgmap:region='${mkgmap:admin_level3}' }
mkgmap:region!=* & is_in:county=* { set mkgmap:region='${is_in:county}' }

mkgmap:city!=* & mkgmap:admin_level8=* { set mkgmap:city='${mkgmap:admin_level8}' }
mkgmap:city!=* & mkgmap:admin_level7=* { set mkgmap:city='${mkgmap:admin_level7}' }
mkgmap:city!=* & mkgmap:admin_level9=* { set mkgmap:city='${mkgmap:admin_level9}' }
mkgmap:city!=* & mkgmap:admin_level10=* { set mkgmap:city='${mkgmap:admin_level10}' }
mkgmap:city!=* & is_in:city=* { set mkgmap:city='${is_in:city}' }
mkgmap:city!=* & addr:city=* { set mkgmap:city='${addr:city}' }

mkgmap:postal_code!=* & mkgmap:postcode=* { set mkgmap:postal_code='${mkgmap:postcode}' }
mkgmap:postal_code!=* & addr:postcode=* { set mkgmap:postal_code='${addr:postcode}' }

mkgmap:street!=* & addr:street=* { set mkgmap:street='${addr:street}' }
mkgmap:street!=* & addr:housename=* { set mkgmap:street='${addr:housename}' }

mkgmap:housenumber!=* & addr:housenumber=* { set mkgmap:housenumber='${addr:housenumber}' }

mkgmap:phone!=* & phone=* { set mkgmap:phone='${phone}' }

mkgmap:is_in!=* & is_in=* { set mkgmap:is_in='${is_in}' }


# ------------------------------------------------------------
# Tourismus / tourism:
# - viele Berghütten sind zusätzlich als Restaurant getaggt
# - weitere Hüttentypen (basic_hut, ...) sind zusätzlich als "shelter" getaggt
# - deshalb erfolgt hier die Verarbeitung vor "amenity"
# ------------------------------------------------------------

# Schutzhütte, Berghütte. [point, closedway]
# siehe auch "amenity = shelter"
# Bewirtschaftete Berghütten
tourism = alpine_hut & name = * & ele = * & amenity = restaurant {name '${name} (${ele})'}   [0x4803 resolution 23]
tourism = alpine_hut & name = * & amenity = restaurant           {name '${name}'}            [0x4803 resolution 23]
tourism = alpine_hut & ele = * & amenity = restaurant            {name 'Berghütte (${ele})'} [0x4803 resolution 23]
tourism = alpine_hut & amenity = restaurant                      {name 'Berghütte'}          [0x4803 resolution 23]

# Selbstversorger Berghütten
tourism = alpine_hut & name = * & ele = * {name '${name} (${ele})'}   [0x4804 resolution 23]
tourism = alpine_hut & name = *           {name '${name}'}            [0x4804 resolution 23]
tourism = alpine_hut & ele = *            {name 'Berghütte (${ele})'} [0x4804 resolution 23]
tourism = alpine_hut                      {name 'Berghütte'}          [0x4804 resolution 23]

# Aquarium.
tourism = aquarium & name = * {name '${name} (Aquarium)'} [0x2c07 resolution 24]

# Sehenswürdigkeit. [point, closedway]
tourism = attraction & name = * {name '${name} (Attraktion)'} [0x6600 resolution 24]

# Kunstwerk, Kunstobjekt. [point, closedway]
tourism = artwork & name = * {name '${name} (Kunstobjekt)'} [0x6600 resolution 24]

# Basic hut (Schutzhütte, Berghütte). [point, closedway]
# A basic hut is a building located in the mountains or into the wild intended
# to provide basic shelter, sleeping, to hikers and climbers.
tourism = basic_hut & name = * & ele = * {name '${name} (${ele})'}   [0x4805 resolution 23]
tourism = basic_hut & name = *           {name '${name}'}            [0x4805 resolution 23]
tourism = basic_hut & ele = *            {name 'Basic Hut (${ele})'} [0x4805 resolution 23]
tourism = basic_hut                      {name 'Basic Hut'}          [0x4805 resolution 23]

# Bed & Breakfast. [point, closedway]
tourism = bed_and_breakfast & name = * { name '${name} (Bed & Breakfast)' } [0x2b02 resolution 24]

# Campingplatz. [point, closedway]
tourism = camp_site & name = * {name '${name} (Camping)'} [0x2b03 resolution 24]

# Stellplatz für Wohnmobile (Reisemobile). [point, closedway]
tourism = caravan_site & name = * {name '${name} (Caravan)'} [0x2b03 resolution 24]

# Ferienwohnung, Ferienhaus, Sennhütte. [point, closedway]
tourism = chalet & name = * { name '${name} (Ferienhaus)' } [0x2b02 resolution 24]

# Pension, Gasthaus. [point, closedway]
tourism = guest_house & name = * { name '${name} (Gasthaus)' } [0x2b02 resolution 24]

# Jugendherberge, Herberge, Naturfreundehaus usw. [point, closedway]
tourism = hostel & name = * { name '${name} (Herberge)' } [0x2b02 resolution 24]

# Hotel. [point, closedway]
tourism = hotel & name = * { name '${stars}* ${name} (Hotel)' | '${name} (Hotel)' } [0x2b01 resolution 24]

# Touristeninformation. [point, closedway]
tourism = information [0x661a resolution 24]

# Lean-to (Schutzhütte, Berghütte). [point, closedway]
# A lean-to is a building located in the mountains or into the wild intended to provide shelter against rain.
tourism = lean_to & name = * & ele = * {name '${name} (${ele})'} [0x4807 resolution 23]
tourism = lean_to & name = *           {name '${name}'}          [0x4807 resolution 23]
tourism = lean_to & ele = *            {name 'Lean-to (${ele})'} [0x4807 resolution 23]
tourism = lean_to                      {name 'Lean-to'}          [0x4807 resolution 23]

# Motel. [point, closedway]
tourism = motel & name = * { name '${stars}* ${name} (Motel)' | '${name} (Motel)' } [0x2b01 resolution 24]

# Museum.
tourism = museum & name = * {name '${name} (Museum)'} [0x2c02 resolution 24]

# Rastplatz (Picknick). [point, closedway]
tourism = picnic_site {name '${name} (Picknick)' | 'Picknick'} [0x4a00 resolution 24]

# Freizeitpark, Themenpark. [point, closedway]
tourism = theme_park & name = * {name '${name} (Freizeitpark)'} [0x2C01 resolution 24]

# Aussichtspunkt. [point]
tourism = viewpoint [0x5200 resolution 24]

# Wilderness hut (Schutzhütte, Berghütte). [point, closedway]
# A wilderness hut is a building located in the mountains or into the wild intended
# to provide shelter, heating to hikers and climbers.
tourism = wilderness_hut & name = * & ele = * {name '${name} (${ele})'}        [0x4806 resolution 23]
tourism = wilderness_hut & name = *           {name '${name}'}                 [0x4806 resolution 23]
tourism = wilderness_hut & ele = *            {name 'Wilderness Hut (${ele})'} [0x4806 resolution 23]
tourism = wilderness_hut                      {name 'Wilderness Hut'}          [0x4806 resolution 23]

# Zoologischer Garten (Zoo), Tierpark. [point, closedway]
tourism = zoo & name = * {name '${name} (Zoo)'} [0x2C07 resolution 24]


# ------------------------------------------------------------
# Natur / natural:
# - vor Ortsangaben (place) verarbeiten
# ------------------------------------------------------------

# Meeresbucht. [point, closedway]
natural = bay & name = * {name '${name} (Meeresbucht)'} [0x6503 resolution 24]

# Strand. [point, closedway]
natural = beach & name = * {name '${name} (Strand)'} [0x6604 resolution 24]

# Eingang zu einer Höhle. [point, closedway]
natural = cave_entrance {name '${name} (Höhle)'} [0x6601 resolution 24]

# Klippe. [point, way, closedway]
natural = cliff & name = * {name '${name} (Klippe)'} [0x6607 resolution 23]

# Joch, Sattel. [point] (s.a. natural = peak)
# 0x01011b = 1
# col -    A high, narrow, sharp-edged pass or saddle through a divide or between two adjacent peaks in a mountain
#          range; especially a deep pass formed by the headward erosion and intersection of two cirques.
# saddle - A low point on a ridge or interfluve, generally a divide (pass, col) between the heads of streams
#          flowing in opposite directions. 
(natural = col | natural = saddle) & name = * & ele = * {name '${name} (${ele})'} [0x661b resolution 23]
(natural = col | natural = saddle) & name = *           {name '${name}'}          [0x661b resolution 23]
(natural = col | natural = saddle) & ele = *            {name 'N.N. (${ele})'}    [0x661b resolution 23]
(natural = col | natural = saddle)                      {name 'N.N.'}             [0x661b resolution 23]

# Alm, Matten: Bereich oberhalb der Baumgrenze. [point, closedway]
natural = fell & name = * {name '${name} (Alm, Matten)'} [0x6600 resolution 24]

# Gebiet (auch Bergspitze), das ganzjährig mit Eis bedeckt ist. [point, closedway]
natural = glacier & name = * {name '${name} (Gletscher)'} [0x650a resolution 24]

# Heide. [point, closedway]
natural = heath & name = * {name '${name} (Heide)'} [0x6600 resolution 24]

# Marsch (Feuchtgebiet, Sumpf, Moor, etc.) veraltet, siehe natural=wetland). [point, closedway]
natural = marsh & name = * {name '${name} (Marsch)'} [0x6513 resolution 24]

# Schlick / Moor. [point, closedway]
natural = mud & name = * {name '${name} (Schlick/Moor)'} [0x6500 resolution 24]

# Bergspitze. [point]
natural = peak & name = * & ele = * {name '${name} (${ele})'} [0x6616 resolution 23]
natural = peak & name = *           {name '${name}'}          [0x6616 resolution 23]
natural = peak & ele = *            {name 'N.N. (${ele})'}    [0x6616 resolution 23]
natural = peak                      {name 'N.N.'}             [0x6616 resolution 23]

# Boden mit Sand bedeckt, kein oder nur spärlicher Bewuchs. [point, closedway]
natural = sand & name = * {name '${name} (Sand)'} [0x6600 resolution 24]

# (Loses) Geröll. [point, closedway]
natural = scree & name = * {name '${name} (Geröll)'} [0x6600 resolution 24]

# Unkultiviertes Land: Unterholz / Busch. [point, closedway]
natural = scrub & name = * {name '${name} (Unkultiviertes Land)'} [0x6600 resolution 24]

# Quelle. [point]
natural = spring {name '${name} (Quelle)' | 'Quelle'} [0x6511 resolution 24]

# Großer Stein, Findling. [point]
natural = stone {name '${name} (Findling)' | 'Findling'} [0x6614 resolution 24]

# Einzelner oder signifikanter Baum. [point]
# Der Baum muss als Landmarke ausgewiesen sein.
natural = tree & denotation = landmark {name '${name} (Baum)' | 'Baum'} [0x6619 resolution 24]

# Vulkan, entweder ruhend, erloschen oder aktiv. [point]
natural = volcano {name '${name} (Vulkan)' | 'Vulkan'} [0x2c0c resolution 24]

# Wasser, Seen, Teiche usw. [point, closedway]
natural = water & name = * {name '${name} (See, Teich)'} [0x650D resolution 24]

# Wasserfall. [point]
natural = waterfall {name '${name} (Wasserfall)' | 'Wasserfall'} [0x6508 resolution 24]

# Feuchtgebiet wie Sumpf, Moor, Ried, Marsch, Aue, Röhricht, Watt, Salzwiese oder Mangrove.
# Naturbelassene Flächen, die Überflutungen ausgesetzt sind oder staunasse Böden aufweisen. [point, closedway]
natural = wetland & name = * {name '${name} (Feuchtgebiet)'} [0x6513 resolution 24]

# Naturwald oder vollkommen unbewirtschaftete Wälder. [point, closedway]
natural = wood & name = * {name '${name} (Naturwald)'} [0x660A resolution 24]


# ------------------------------------------------------------
# Ortsangaben / place:
# ------------------------------------------------------------

# Kontinent. [point, closedway]
# place = continent (wird derzeit nicht unterstützt)

# Staat. [point]
# place = country (wird derzeit nicht unterstützt)

# Bundesland, Kanton. [point, closedway]
# place = state (wird derzeit nicht unterstützt)

# Region, Regierungsbezirk. [point, closedway]
# place = region (wird derzeit nicht unterstützt)

# Landkreis, Grafschaft, Bezirk. [point, closedway]
# place = county (wird derzeit nicht unterstützt)

# Megastadt (>1.000.000 Einwohner) oder Landeshauptstadt. [point, closedway]
place = city & (population > 1000000 | capital = true) [0x0100 resolution 17]

# Großstadt (wie von der zuständigen Behörde definiert, häufig > 100.000 Einwohner),
# Landeshauptstadt, Provinzhauptstadt. [point, closedway]
place = city [0x0200 resolution 17]

# Weiler, Bauernschaft, Einöde (Ansammlung einiger Häuser). [point, closedway]
place = hamlet [0x0d00 resolution 24]

# "Bedeutende" Inseln werden auch in niedrigen Zoomstufen ausgewiesen.

# Insel (Nordsee, Helgoland). [point, closedway]
place = island & (name = Helgoland) [0x0600 resolution 18]

# Insel (Nordsee, Nordfriesische Inseln). [point, closedway]
place = island & (name = Amrum | name = Föhr | name = Pellworm | name = Sylt) [0x0600 resolution 18]

# Insel (Nordsee, Ostfriesische Inseln). [point, closedway]
place = island & (name = Baltrum | name = Borkum | name = Juist | name = Langeoog | name = Mellum | name = Norderney | name = Spiekeroog | name = Wangerooge) [0x0600 resolution 18]

# Insel (Ostsee). [point, closedway]
place = island & (name = Fehmarn | name = Hiddensee | name = Poel | name = Rügen | name = Ummanz | name = Usedom) [0x0600 resolution 18]

# Insel (Binnenland). [point, closedway]
place = island & (name = Harriersand) [0x0600 resolution 18]

# Insel (allgemein). [point, closedway] (Signatur analog 'hamlet')
place = island & name = * [0x0d00 resolution 23]

# An islet is any small, possibly hamlet sized piece of land that is completely surrounded by water
# and isolated from other significant landmasses. [point, closedway]
place = islet & name = * {name '${name} (kleine Insel)'} [0x6600 resolution 24]

# Isolated_dwelling: Smallest rural settlement, smaller than a hamlet, usually just one house. [point, closedway]
place = isolated_dwelling & name = * {name '${name} (Einöde)'} [0x6600 resolution 24]

# Ortsbezeichnung ohne Bezug auf Bevölkerung (z.B. "Wiesengrund", "Deutsches Eck"). [point, closedway]
#-# place = locality & name = * {name '${name} (Ortsbezeichnung)'} [0x6600 resolution 24]
place = locality & name = * & ele = * {name '${name} (${ele}) (Ortsbezeichnung)'} [0x6600 resolution 23]
place = locality & name = *           {name '${name} (Ortsbezeichnung)'}          [0x6600 resolution 23]
place = locality & ele = *            {name 'N.N. (${ele}) (Ortsbezeichnung)'}    [0x6600 resolution 23]
place = locality                      {name 'N.N. (Ortsbezeichnung)'}             [0x6600 resolution 23]

# Stadtteil, Stadtviertel. [point, closedway]
place = suburb [0x0b00 resolution 21]

# Stadt, Markt, selbständige Gemeinde (wie von der zuständigen Behörde definiert),
# in ländlichen Regionen > 2.000 Einwohner; in Ballungsräumen > 10.000 Einwohner. [point, closedway]
place = town [0x0600 resolution 18]

# Dorf (< 2.000 Einwohner), ehemals selbständige Gemeinde. [point, closedway]
place = village [0x0c00 resolution 21]


# ------------------------------------------
# Energieversorgung / power:
# ------------------------------------------

# Strommast: Mast für Hochspannungsüberlandleitungen. [point]
power = tower {name '${name} (Strommast)' | 'Strommast'} [0x6417 resolution 24]

# Strommast für Nieder- und Mittelspannungsleitungen, oft aus Holz oder Beton. [point]
# power = pole (wird derzeit nicht unterstützt)

# Umspannwerk: bezeichnet eine Umspannanlage, die ein Höchst- oder Hochspannungs- mit einem weiteren
# Hochspannungs- oder einem Mittelspannungsnetz verbindet. [point, closedway]
power = station & name = * {name '${name} (Umspannwerk)'} [0x6600 resolution 24]

# Umspannstation, kleiner als station, verbindet ein Mittel- mit einem Niederspannungsnetz,
# z.B. Mini-Umspannwerk, Trafohäuschen oder großer Trafokasten. [point, closedway]
power = station & name = * {name '${name} (Umspannstation)'} [0x6600 resolution 24]

# Windenergieanlage. [point, closedway]
power = generator & power_source = wind {name '${name} (Windenergieanlage)' | 'Windenergieanlage'} [0x6419 resolution 24]

# Kraftwerk: dient der Erzeugung von Strom oder Wärme. [point, closedway]
power = generator & name = * {name '${name} (Kraftwerk)'} [0x6600 resolution 24]

# Kabelverteilerschrank: in diesen Schränken werden die 400V-Stromkreise zu den Hausanschlüssen,
# Anschluss- und Übergabepunkten der regionalen Energieversorger verzweigt. Weitere Einsatzgebiete
# sind Verteilungen der kommunalen Straßenbeleuchtung oder Anschluss- und Übergabepunkte
# für Schausteller usw. [point]
# power = cable_distribution_cabinet (wird derzeit nicht unterstützt)


