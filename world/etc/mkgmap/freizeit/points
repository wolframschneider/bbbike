# ------------------------------------------------------------
# Nutzung / Einrichtung / amenity:
# ------------------------------------------------------------

# Kategorie "Verpflegung, Einkehr"
# --------------------------------

# Bar, Nachtlokal. Es werden hauptsächlich alkoholische Getränke serviert. [point, closedway]
amenity = bar & name = * {name '${name} (Bar)'}  [0x2d02 resolution 24]

# Biergarten. [point, closedway]
amenity = biergarten & name = * {name '${name} (Biergarten)'} [0x2d02 resolution 24]

# Café, Bistro. [point, closedway]
amenity = cafe & name = * {name '${name} (Café)'} [0x2a0e resolution 24]

# Schnell-Restaurant, Imbiss. Die Art kann mit cuisine=* angegeben werden, als Wert z.B. =burger
# für ein Burgerrestaurant oder =kebab für einen Dönerimbiss. [point, closedway]
amenity = fast_food & cuisine = grill & name = * {name '${name} (${cuisine}, Fastfood)'} [0x2a03 resolution 24]
amenity = fast_food & cuisine = pizza & name = * {name '${name} (${cuisine}, Fastfood)'} [0x2a0a resolution 24]
amenity = fast_food & cuisine = *     & name = * {name '${name} (${cuisine}, Fastfood)'} [0x2a07 resolution 24]
amenity = fast_food                   & name = * {name '${name} (Fastfood)'}             [0x2a07 resolution 24]

# Eiscafé, Eisdiele. [point, closedway]
amenity = ice_cream & name = * {name '${name} (Eiscafé)'} [0x2a0e resolution 24]

# Kneipe. [point, closedway]
amenity = pub & name = * {name '${name} (Kneipe)'}  [0x2d02 resolution 24]

# Restaurant (ohne Fast-Food). Die Art der Küche kann mit cuisine=* angegeben werden,
# als Wert z.B. "italian" für italienische oder "greek" für griechische Küche. [point, closedway]

# Amerikanisch.
amenity = restaurant & cuisine = american & name = * {name '${name} (Amerikanisch, Restaurant)'} [0x2a01 resolution 24]
amenity = restaurant & cuisine = burger   & name = * {name '${name} (Hamburger, Restaurant)'} [0x2a01 resolution 24]

# Asiatisch.
amenity = restaurant & cuisine = thai       & name = * {name '${name} (Thailändisch, Restaurant)'} [0x2a02 resolution 24]
amenity = restaurant & cuisine = asian      & name = * {name '${name} (Asiatisch, Restaurant)'} [0x2a02 resolution 24]
amenity = restaurant & cuisine = japanese   & name = * {name '${name} (Japanisch, Restaurant)'} [0x2a02 resolution 24]
amenity = restaurant & cuisine = korean     & name = * {name '${name} (Koreanisch, Restaurant)'} [0x2a02 resolution 24]
amenity = restaurant & cuisine = vietnamese & name = * {name '${name} (Vietnamesisch, Restaurant)'} [0x2a02 resolution 24]

# Grillen.
amenity = restaurant & cuisine = grill    & name = * {name '${name} (${cuisine}, Restaurant)'} [0x2a03 resolution 24]
amenity = restaurant & cuisine = barbecue & name = * {name '${name} (${cuisine}, Restaurant)'} [0x2a03 resolution 24]

# Chinesisch.
amenity = restaurant & cuisine = chinese & name = * {name '${name} (Chinesisch, Restaurant)'} [0x2a04 resolution 24]

# International.
amenity = restaurant & cuisine = international & name = * {name '${name} (${cuisine}, Restaurant)'} [0x2a06 resolution 24]

# Fastfood.
